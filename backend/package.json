{
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "build": "tsc && tsc-alias",
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register --ignore-watch ../frontend/.next src/server.ts",
    "seed": "node --max-old-space-size=6144 -r ts-node/register src/prisma/seed/fromTo.ts"
  },
  "prisma": {
    "schema": "src/prisma"
  },
  "type": "commonjs",
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "js,ts",
    "execMap": {
      "ts": "ts-node"
    }
  },
  "dependencies": {
    "@prisma/adapter-mariadb": "^7.0.1",
    "@prisma/client": "^7.0.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/nodemailer": "^6.4.17",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cross-env": "^10.1.0",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "express-validator": "^7.2.1",
    "google-auth-library": "^10.2.1",
    "googleapis": "^155.0.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "next": "^16.0.1",
    "nodemailer": "^7.0.5",
    "pg-promise": "^11.15.0",
    "postgres": "^3.4.7",
    "prisma": "^7.0.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "socket.io": "^4.8.1",
    "tsc-alias": "^1.8.16",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/express-session": "^1.18.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.1",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}
