model Country {
    id          Int     @id @default(autoincrement())
    code        String
    engName     String
    iso2        String  @unique
    countryCode String? @unique
    user        User[]
    city        City[]
    state       State[]
}

model City {
    id         Int     @id @unique
    nationIso2 String
    countries  Country @relation(fields: [nationIso2], references: [iso2])
    country    String
    state      String
    city       String
}

model State {
    id         Int     @id @unique
    nationIso2 String
    countries  Country @relation(fields: [nationIso2], references: [iso2])
    country    String
    state      String
}
